SELECT 
    CASE 
        WHEN CHARINDEX('?', url) > 0 THEN LEFT(url, CHARINDEX('?', url) - 1)
        ELSE LEFT(url, LEN(url) - PATINDEX('%/[0-9]%', REVERSE(url) + '/') + 2)
    END AS grouped_url,
    COUNT(*) AS url_count
FROM your_table
GROUP BY 
    CASE 
        WHEN CHARINDEX('?', url) > 0 THEN LEFT(url, CHARINDEX('?', url) - 1)
        ELSE LEFT(url, LEN(url) - PATINDEX('%/[0-9]%', REVERSE(url) + '/') + 2)
    END;
